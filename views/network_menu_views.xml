<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <menuitem id="menu_network_management" name="Network Management" sequence="10"/>

    <menuitem id="menu_subnet" name="Subnets" parent="menu_network_management" sequence="10" action="action_subnet" />
    <menuitem id="menu_device" name="Devices" parent="menu_network_management" sequence="20" action="action_device" />
    <menuitem id="menu_partners" name="Users" parent="menu_network_management" sequence="50" action="action_partner_custom_list"/>

    <!-- Server Action to Trigger Import -->
    <record id="action_server_network_import" model="ir.actions.server">
        <field name="name">Import Network Data</field>
        <field name="model_id" ref="model_network_import"/>
        <field name="state">code</field>
        <field name="code">model.action_import()</field>
    </record>

    <menuitem id="menu_network_import" name="Run Netadmin Import" parent="menu_network_management" action="action_server_network_import" sequence="100"/>
</odoo>